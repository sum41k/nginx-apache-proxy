<VirtualHost 127.0.0.1:8080>
        ServerName bigdick.com
        ServerAlias www.bigdick.com
        DocumentRoot /var/www/sum41k/data/www/bigdick.com
        ServerAdmin webmaster@bigdick.com
        DirectoryIndex index.php index.html
        AddDefaultCharset off
        #AssignUserID sum41k sum41k
        CustomLog /var/www/httpd-logs/bigdick.com.access.log combined
        ErrorLog /var/www/httpd-logs/bigdick.com.error.log
        <FilesMatch "\.ph(p[3-5]?|tml)$">
                SetHandler application/x-httpd-php
        </FilesMatch>
        <FilesMatch "\.phps$">
                SetHandler application/x-httpd-php-source
        </FilesMatch>
        <IfModule php5_module>
                php_admin_value sendmail_path "/usr/sbin/sendmail -t -i -f webmaster@creditcentr.biz"
                php_admin_value upload_tmp_dir "/var/www/sum41k/data/mod-tmp"
                php_admin_value session.save_path "/var/www/sum41k/data/mod-tmp"
                php_admin_value open_basedir "/var/www/sum41k/data:."
        </IfModule>
        <IfModule php7_module>
                php_admin_value sendmail_path "/usr/sbin/sendmail -t -i -f webmaster@creditcentr.biz"
                php_admin_value upload_tmp_dir "/var/www/sum41k/data/mod-tmp"
                php_admin_value session.save_path "/var/www/sum41k/data/mod-tmp"
                php_admin_value open_basedir "/var/www/sum41k/data:."
        </IfModule>
        SetEnvIf X-Forwarded-Proto https HTTPS=on
</VirtualHost>
<Directory /var/www/sum41k/data/www/bigdick.com>
        Options +Includes -ExecCGI
        <IfModule php5_module>
                php_admin_flag engine on
        </IfModule>
        <IfModule php7_module>
                php_admin_flag engine on
        </IfModule>
</Directory>
